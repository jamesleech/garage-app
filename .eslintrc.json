{
  "plugins": [
    "react",
    "react-native"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    },
    "allowImportExportEverywhere": false
  },
  "env": {
    "es6": true,
    "react-native/react-native": true
  },
  "extends": [ "plugin:react-native/all" ]
}
